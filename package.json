{
  "name": "@nebula.js/sn-pivot-table",
  "version": "1.0.2",
  "description": "Pivot table supernova",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-pivot-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-pivot-table.git"
  },
  "license": "MIT",
  "author": "QlikTech International AB",
  "main": "dist/sn-pivot-table.js",
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-pivot-table-ext"
  ],
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:dev": "node ./scripts/build.js --core --ext --mode development",
    "build:watch": "node ./scripts/build.js --mode development -w",
    "copy:ext": "node ./scripts/copy-ext.js",
    "format:check": "prettier --check '**' --ignore-unknown",
    "format:write": "prettier --write '**' --ignore-unknown",
    "lint": "eslint src/",
    "prepare": "husky install",
    "sense": "nebula sense",
    "spec": "scriptappy-from-jsdoc -c ./scriptappy.config.js",
    "start": "nebula serve  --type sn-pivot-table",
    "start:mfe": "nebula serve --mfe  --type sn-pivot-table",
    "test:local:rendering": "./test/rendering/scripts/run-rendering-test.sh",
    "test:local:update:screenshots": "./test/rendering/scripts/update-screenshots.sh",
    "test:rendering": "playwright test",
    "test:unit": "jest --runInBand",
    "types:check": "tsc --noEmit",
    "preversion": "yarn build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && yarn spec && git add api-specifications CHANGELOG.md"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'yarn lint'",
      "bash -c 'yarn types:check'"
    ],
    "*": "bash -c 'yarn format:check'"
  },
  "prettier": "@qlik/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "extends": [
      "@qlik/eslint-config-react/typescript",
      "@qlik/eslint-config/jest",
      "@qlik/eslint-config/playwright"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ]
    },
    "root": true
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@nebula.js/cli": "3.3.0",
    "@nebula.js/cli-build": "3.3.0",
    "@nebula.js/cli-sense": "3.3.0",
    "@nebula.js/cli-serve": "3.3.0",
    "@playwright/test": "1.32.3",
    "@qlik/eslint-config": "0.0.9",
    "@qlik/eslint-config-react": "0.0.9",
    "@qlik/prettier-config": "0.1.1",
    "@qlik/tsconfig": "0.0.11",
    "@rollup/plugin-typescript": "11.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/react-window": "1.8.5",
    "babel-eslint": "10.1.0",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.38.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "qlik-chart-modules": "0.54.0",
    "qlik-object-conversion": "0.16.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-window": "1.8.8",
    "scriptappy-from-jsdoc": "0.7.0",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "3.x"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "systemjs": "dist/sn-pivot-table.systemjs.js"
}
