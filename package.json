{
  "name": "@nebula.js/sn-pivot-table",
  "version": "2.1.0",
  "description": "Pivot table supernova",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-pivot-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-pivot-table.git"
  },
  "license": "MIT",
  "author": "QlikTech International AB",
  "main": "dist/sn-pivot-table.js",
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-pivot-table-ext"
  ],
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:dev": "node ./scripts/build.js --core --ext --mode development",
    "build:watch": "node ./scripts/build.js --mode development -w",
    "copy:ext": "node ./scripts/copy-ext.js",
    "format:check": "prettier --check '**' --ignore-unknown",
    "format:write": "prettier --write '**' --ignore-unknown",
    "postinstall": "yarn locale:generate",
    "lint": "eslint src/",
    "locale:generate": "node src/locale/scripts/generate-all.mjs",
    "locale:verify": "node src/locale/tools/verify-translations.mjs",
    "prepare": "husky install",
    "sense": "nebula sense",
    "spec": "npx @scriptappy/cli from-jsdoc -c ./scriptappy.config.js",
    "start": "nebula serve  --type sn-pivot-table",
    "start:fixture": "nebula serve --entry . --type sn-pivot-table --open false --build false --fixturePath test/rendering/__fixtures__",
    "start:mfe": "nebula serve --mfe  --type sn-pivot-table",
    "test:e2e": "yarn test:rendering",
    "test:local:rendering": "./test/rendering/scripts/run-rendering-test.sh",
    "test:local:update:screenshots": "./test/rendering/scripts/update-screenshots.sh",
    "test:rendering": "playwright test",
    "test:unit": "jest",
    "types:check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'yarn lint'",
      "bash -c 'yarn types:check'"
    ],
    "*": "bash -c 'yarn format:check'"
  },
  "prettier": "@qlik/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "extends": [
      "@qlik/eslint-config",
      "@qlik/eslint-config/react",
      "@qlik/eslint-config/jest",
      "@qlik/eslint-config/playwright"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ]
    },
    "root": true
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.11",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.1",
    "@mui/styled-engine-sc": "^5.12.0",
    "@nebula.js/cli": "4.4.0",
    "@nebula.js/cli-build": "4.4.0",
    "@nebula.js/cli-sense": "4.4.0",
    "@nebula.js/cli-serve": "4.4.0",
    "@playwright/test": "1.37.1",
    "@qlik-trial/sprout": "3.4.0",
    "@qlik/eslint-config": "0.4.15",
    "@qlik/nebula-table-utils": "2.0.0",
    "@qlik/prettier-config": "0.3.0",
    "@qlik/tsconfig": "0.1.6",
    "@rollup/plugin-typescript": "11.1.3",
    "@scriptappy/cli": "0.10.0",
    "@scriptappy/from-jsdoc": "0.19.0",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.4",
    "@types/qlik-engineapi": "^12.67.13",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-window": "1.8.5",
    "babel-eslint": "10.1.0",
    "eslint": "8.48.0",
    "globby": "13.2.2",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-junit": "16.0.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.2",
    "qlik-chart-modules": "0.59.0",
    "qlik-object-conversion": "0.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-window": "1.8.9",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "^4.4.0",
    "@qlik-trial/sprout": "^3.4.0"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "systemjs": "dist/sn-pivot-table.systemjs.js"
}
