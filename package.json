{
  "name": "@nebula.js/sn-pivot-table",
  "version": "2.0.0",
  "description": "Pivot table supernova",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-pivot-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-pivot-table.git"
  },
  "license": "MIT",
  "author": "QlikTech International AB",
  "main": "dist/sn-pivot-table.js",
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-pivot-table-ext"
  ],
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:dev": "node ./scripts/build.js --core --ext --mode development",
    "build:watch": "node ./scripts/build.js --mode development -w",
    "copy:ext": "node ./scripts/copy-ext.js",
    "format:check": "prettier --check '**' --ignore-unknown",
    "format:write": "prettier --write '**' --ignore-unknown",
    "postinstall": "yarn locale:generate",
    "lint": "eslint src/",
    "locale:generate": "node src/locale/scripts/generate-all.mjs",
    "locale:verify": "node src/locale/tools/verify-translations.mjs",
    "prepare": "husky install",
    "sense": "nebula sense",
    "spec": "npx @scriptappy/cli from-jsdoc -c ./scriptappy.config.js",
    "start": "nebula serve  --type sn-pivot-table",
    "start:mfe": "nebula serve --mfe  --type sn-pivot-table",
    "test:local:rendering": "./test/rendering/scripts/run-rendering-test.sh",
    "test:local:update:screenshots": "./test/rendering/scripts/update-screenshots.sh",
    "test:rendering": "playwright test",
    "test:unit": "jest",
    "types:check": "tsc --noEmit",
    "preversion": "yarn build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && yarn spec && git add api-specifications CHANGELOG.md"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'yarn lint'",
      "bash -c 'yarn types:check'"
    ],
    "*": "bash -c 'yarn format:check'"
  },
  "prettier": "@qlik/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "extends": [
      "@qlik/eslint-config",
      "@qlik/eslint-config-react/typescript",
      "@qlik/eslint-config/jest",
      "@qlik/eslint-config/playwright"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ]
    },
    "root": true
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.1",
    "@mui/styled-engine-sc": "^5.12.0",
    "@nebula.js/cli": "4.0.3",
    "@nebula.js/cli-build": "4.0.3",
    "@nebula.js/cli-sense": "4.0.3",
    "@nebula.js/cli-serve": "4.0.3",
    "@playwright/test": "1.34.0",
    "@qlik-oss/nebula-table-utils": "1.1.0",
    "@qlik-trial/sprout": "2.51.0",
    "@qlik/eslint-config": "0.0.16",
    "@qlik/eslint-config-react": "0.0.15",
    "@qlik/prettier-config": "0.1.2",
    "@qlik/tsconfig": "0.1.3",
    "@rollup/plugin-typescript": "11.1.1",
    "@scriptappy/cli": "0.8.0",
    "@scriptappy/from-jsdoc": "0.17.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-window": "1.8.5",
    "babel-eslint": "10.1.0",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.41.0",
    "globby": "13.1.4",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "qlik-chart-modules": "0.54.0",
    "qlik-object-conversion": "0.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-window": "1.8.9",
    "tslib": "2.5.2",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=4.0.3",
    "@qlik-trial/sprout": ">=2.51.0"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "systemjs": "dist/sn-pivot-table.systemjs.js"
}
