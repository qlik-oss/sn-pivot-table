{
  "extends": ["qlik-oss", ":dependencyDashboard"],
  "circleci": {
    "ignoreDeps": []
  },
  "semanticCommits": true,
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "depTypeList": ["engines"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@qlik/tsconfig", "@qlik-oss/nebula-table-utils", "@qlik-trial/sprout"],
      "enabled": false
    },
    {
      "rangeStrategy": "bump",
      "matchDepTypes": ["peerDependencies"]
    }
  ]
}
