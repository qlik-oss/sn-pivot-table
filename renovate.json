{
  "extends": ["qlik-oss", ":dependencyDashboard"],
  "circleci": {
    "ignoreDeps": []
  },
  "semanticCommits": true,
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "depTypeList": ["engines"],
      "enabled": false
    },
    {
      "matchPackageNames": [
        "@qlik/eslint-config",
        "@qlik/eslint-config-react",
        "@qlik/prettier-config",
        "@qlik/tsconfig"
      ],
      "enabled": false
    }
  ]
}
