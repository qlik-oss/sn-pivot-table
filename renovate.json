{
  "extends": ["qlik-oss", ":dependencyDashboard"],
  "circleci": {
    "ignoreDeps": []
  },
  "semanticCommits": true,
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "depTypeList": ["engines", "packageManager"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@qlik/nebula-table-utils", "@qlik-trial/sprout", "qlik-chart-modules"],
      "enabled": false
    },
    {
      "enabled": false,
      "matchDepTypes": ["peerDependencies"]
    }
  ]
}
